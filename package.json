{
  "name": "@enpago/events",
  "description": "Events for Enpago",
  "version": "0.0.4",
  "type": "module",
  "author": "NR1E, Inc",
  "publishConfig": {
    "access": "public"
  },
  "license": "See LICENSE file",
  "scripts": {
    "prebuild": "prettier --check . && eslint .",
    "build": "tsc",
    "postbuild": "cp LICENSE README.md package.json dist && pnpm run test",
    "watch": "tsc -w",
    "test": "vitest run --coverage",
    "clean": "rm -rf dist && rm -rf site && rm -rf coverage",
    "fmt": "prettier --write .",
    "generate": "graphql-codegen --config codegen.yml",
    "site": "typedoc --out site \"src/**/*.ts\" --exclude \"src/**/*.test.ts\""
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.4.1",
    "@types/node": "22.9.3",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.20.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.15.0",
    "valibot": "^0.42.1",
    "vitest": "^1.6.0"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "types": "./index.d.mts"
    },
    "./types": {
      "import": "./types/index.d.mts",
      "types": "./types/index.d.mts"
    }
  },
  "packageManager": "pnpm@10.8.0"
}
